<?xml version='1.0' encoding='utf-8'?>
<interface version="1.2">
  <object class="tk.Frame" id="room_tab">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
      <property name="sticky">nsew</property>
    </layout>
    <containerlayout manager="grid">
      <property type="col" id="0" name="weight">1</property>
      <property type="row" id="1" name="weight">1</property>
    </containerlayout>
    <child>
      <object class="ttk.Frame" id="room_controls">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="row">0</property>
          <property name="sticky">new</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="0" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="tk.LabelFrame" id="controls_frame">
            <property name="height">1</property>
            <property name="text" translatable="yes">Controls</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
            <child>
              <object class="tk.Button" id="leave_button">
                <property name="bitmap">error</property>
                <property name="relief">flat</property>
                <property name="text" translatable="yes">Leave</property>
                <layout manager="grid">
                  <property name="column">0</property>
                  <property name="row">0</property>
                  <property name="sticky">w</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="tk.LabelFrame" id="health_frame">
            <property name="height">1</property>
            <property name="pady">2</property>
            <property name="text" translatable="yes">Health</property>
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
            </layout>
            <child>
              <object class="ttk.Label" id="health_label">
                <property name="image">healthy.png</property>
                <layout manager="pack">
                  <property name="side">top</property>
                </layout>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="pygubu.builder.widgets.tkscrollbarhelper" id="chat_scroll">
        <property name="scrolltype">vertical</property>
        <property name="takefocus">false</property>
        <property name="usemousewheel">true</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
          <property name="sticky">nsew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="pygubu.builder.widgets.tkscrollbarhelper" id="userlist_scroll">
        <property name="scrolltype">vertical</property>
        <property name="takefocus">false</property>
        <property name="usemousewheel">true</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">1</property>
          <property name="row">1</property>
          <property name="sticky">nsew</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="inputs_frame">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="columnspan">2</property>
          <property name="row">2</property>
          <property name="sticky">sew</property>
        </layout>
        <containerlayout manager="grid">
          <property type="col" id="1" name="weight">1</property>
        </containerlayout>
        <child>
          <object class="ttk.Button" id="attach_button">
            <property name="text" translatable="yes">Attach</property>
            <layout manager="grid">
              <property name="column">0</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="ttk.Button" id="send_button">
            <property name="text" translatable="yes">Send</property>
            <layout manager="grid">
              <property name="column">2</property>
              <property name="row">0</property>
              <property name="sticky">nsew</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="tk.Text" id="text_entry">
            <property name="borderwidth">2</property>
            <property name="exportselection">true</property>
            <property name="height">2</property>
            <property name="relief">groove</property>
            <property name="takefocus">true</property>
            <property name="undo">true</property>
            <property name="width">50</property>
            <bind sequence="&lt;Alt-KeyPress-Return&gt;" handler="insert_newline" add="True" />
            <bind sequence="&lt;KeyPress-Return&gt;" handler="send_message" add="" />
            <layout manager="grid">
              <property name="column">1</property>
              <property name="row">0</property>
              <property name="sticky">ew</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
